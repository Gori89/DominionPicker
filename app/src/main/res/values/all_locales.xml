<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingTranslation"
    translatable="false" >
    <!-- String resources that must be available in all locales
         so that the cards in non-default languages can display. -->


    <!-- languages ordered alphabetically -->
    <string-array name="language_names">
        <item>@string/system_default</item>
        <item>Deutsch</item>
        <item>English</item>
        <item>Fran&#231;ais</item>
        <item>Italiano</item>
        <item>Nederlands</item>
        <item>Portugu&#234;s</item>
    </string-array>
    <string-array name="language_codes">
        <item>0</item>
        <item>de</item>
        <item>en</item>
        <item>fr</item>
        <item>it</item>
        <item>nl</item>
        <item>pt</item>
    </string-array>


    <!-- String used to describe coins -->
    <array name="coin">
        <item>@string/coin</item>
        <item>M&#252;nze</item>
        <item>Coin</item>
        <item>Pièce</item>
        <item>Moneta</item>
        <item>Munt</item>
        <item>Moeda</item>
    </array>
    <array name="format_coin">
        <item>@plurals/format_coin</item>
        <item>@plurals/format_coin_de</item>
        <item>@plurals/format_coin_en</item>
        <item>@plurals/format_coin_fr</item>
        <item>@plurals/format_coin_it</item>
        <item>@plurals/format_coin_nl</item>
        <item>@plurals/format_coin_pt</item>
    </array>
    <plurals name="format_coin_en">
        <item quantity="one">%1$s Coin</item>
        <item quantity="other">%1$s Coins</item>
    </plurals>
    <plurals name="format_coin_de">
        <item quantity="one">%1$s M&#252;nze</item>
        <item quantity="other">%1$s M&#252;nzen</item>
    </plurals>
    <plurals name="format_coin_fr">
        <item quantity="one">%1$s Pièce</item>
        <item quantity="other">%1$s Pièces</item>
    </plurals>
    <plurals name="format_coin_it">
        <item quantity="one">%1$s Moneta</item>
        <item quantity="other">%1$s Monete</item>
    </plurals>
    <plurals name="format_coin_nl">
        <item quantity="one">%1$s Munt</item>
        <item quantity="other">%1$s Munten</item>
    </plurals>
    <plurals name="format_coin_pt">
        <item quantity="one">%1$s Moeda</item>
        <item quantity="other">%1$s Moedas</item>
    </plurals>


    <!-- String used to describe potions -->
    <string-array name="potion">
        <item>@string/potion</item>
        <item>Trank</item>
        <item>Potion</item>
        <item>Potion</item>
        <item>Pozione</item>
        <item>Toverdrank</item>
        <item>Po&#231;&#227;o</item>
    </string-array>
    <array name="format_potion">
        <item>@plurals/format_potion</item>
        <item>@plurals/format_potion_de</item>
        <item>@plurals/format_potion_en</item>
        <item>@plurals/format_potion_fr</item>
        <item>@plurals/format_potion_it</item>
        <item>@plurals/format_potion_nl</item>
        <item>@plurals/format_potion_pt</item>
    </array>
    <plurals name="format_potion_en">
        <item quantity="one">%1$s Potion</item>
        <item quantity="other">%1$s Potions</item>
    </plurals>
    <plurals name="format_potion_de">
        <item quantity="one">%1$s Trank</item>
        <item quantity="other">%1$s Tr&#228;nke</item>
    </plurals>
    <plurals name="format_potion_fr">
        <item quantity="one">%1$s Potion</item>
        <item quantity="other">%1$s Potions</item>
    </plurals>
    <plurals name="format_potion_it">
        <item quantity="one">%1$s Pozione</item>
        <item quantity="other">%1$s Pozioni</item>
    </plurals>
    <plurals name="format_potion_nl">
        <item quantity="one">%1$s Toverdrank</item>
        <item quantity="other">%1$s Toverdrankjes</item>
    </plurals>
    <plurals name="format_potion_pt">
        <item quantity="one">%1$s Po&#231;&#227;o</item>
        <item quantity="other">%1$s Po&#231;&#245;es</item>
    </plurals>


    <!-- String used to describe victory points -->
    <array name="vp">
        <item>@string/vp</item>
        <item>M&#252;nze</item>
        <item>Coin</item>
        <item>Point de Victoire</item>
        <item>Punti Vittoria</item>
        <item>Winstpunt</item>
        <item>Ponto de Vit&#243;ria</item>
    </array>
    <array name="format_vp">
        <item>@plurals/format_vp</item>
        <item>@plurals/format_vp_de</item>
        <item>@plurals/format_vp_en</item>
        <item>@plurals/format_vp_fr</item>
        <item>@plurals/format_vp_it</item>
        <item>@plurals/format_vp_nl</item>
        <item>@plurals/format_vp_pt</item>
    </array>
    <plurals name="format_vp_en">
        <item quantity="one">%1$s Victory Point</item>
        <item quantity="other">%1$s Victory Points</item>
    </plurals>
    <plurals name="format_vp_de">
        <item quantity="one">%1$s Siegpunkt</item>
        <item quantity="other">%1$s Siegpunkte</item>
    </plurals>
    <plurals name="format_vp_fr">
        <item quantity="one">%1$s Point de Victoire</item>
        <item quantity="other">%1$s Points de Victoire</item>
    </plurals>
    <plurals name="format_vp_it">
        <item quantity="one">%1$s Punto Vittoria</item>
